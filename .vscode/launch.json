{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Django Backend",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/octofit-tracker/backend/octofit_tracker/manage.py",
      "args": ["runserver", "0.0.0.0:8000", "--noreload"],
      "django": true,
      "justMyCode": true,
      "python": "${workspaceFolder}/octofit-tracker/backend/venv/bin/python",
      "cwd": "${workspaceFolder}/octofit-tracker/backend/octofit_tracker",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/octofit-tracker/backend/octofit_tracker:${workspaceFolder}/octofit-tracker/backend/venv/lib/python3.10/site-packages",
        "VIRTUAL_ENV": "${workspaceFolder}/octofit-tracker/backend/venv",
        "PATH": "${workspaceFolder}/octofit-tracker/backend/venv/bin:${env:PATH}",
        "DJANGO_SETTINGS_MODULE": "octofit_tracker.settings"
      }
    },
    {
      "name": "Launch React Frontend",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/octofit-tracker/frontend/node_modules/react-scripts/scripts/start.js",
      "cwd": "${workspaceFolder}/octofit-tracker/frontend",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["start"],
      "port": 3000,
      "env": {
        "REACT_APP_CODESPACE_NAME": "${env:CODESPACE_NAME}"
      }
    }
  ]
}